<UserControl x:Class="ClientApp.SelectContact"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ClientApp"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="200">
    <Grid>
        <WrapPanel HorizontalAlignment="Left" VerticalAlignment="Center">
            <CheckBox Name="cbContact" Grid.Column="0"/>
            <TextBlock Name="txbContactName"   Grid.Column="1" VerticalAlignment="Bottom" Foreground="White" FontFamily="Segoe UI Light" FontSize="11" Margin="8,0,0,0">Contact Name</TextBlock>
        </WrapPanel>
    </Grid>
</UserControl>
